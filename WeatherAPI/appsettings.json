{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "RequestsPerWindow": 3,
    "WindowInSeconds": 3,
    "ConsecutiveLimitForBlock": 3,
    "BlockDurationInMinutes": 1,
    "EnableBlocking": true
  }
}
